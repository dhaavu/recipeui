a>\n\n  </div>\n  <div class=\"menu\">\n    <ul>\n      <li v-for=\"menuItem in menuItems\" :key=\"menuItem.name\">{{menuItem.name}}</li>\n      \n    </ul>\n  </div>\n  <div class=\"profile\">\n    <a @click=\"expandMenu\" class=\"material-icons\">menu</a>\n    <ul>\n      <li>Register</li>\n      <li> | </li>\n      <li>Login</li>\n    </ul>\n    \n    \n   </div>\n  \n\n\n</div>\n\n<div v-if=\"!isHidden\" class=\"mobileMenu\">\n  <div v-for=\"menuItem in menuItems\" :key =\"menuItem.name\"> {{menuItem.name}}</div>\n</div>\n\n</template>\n\n<script>\nexport default {\n  name: 'Header',\n  props: {\n     menuItems: Array, \n      logoName: String \n  }, \n  data(){\n    return {\n     isHidden: true\n\n    }\n  }, \n  methods: {\n    expandMenu(event){\n      event.preventDefault(); \n      console.log(\"clicked\"); \n      this.isHidden = !this.isHidden; \n    }\n  }\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\nh3 {\n  margin: 40px 0 0;\n}\nul {\n  list-style-type: none;\n  padding: 0;\n}\nli {\n  display: inline-block;\n  margin: 0 10px;\n}\na {\n  border-radius:50%; \n  border: white 1px solid; \n  cursor:pointer;\n}\n\n.header{\n  background: #42b983; \n  display: flex; \n  justify-content:space-around;\n  align-items: center;\n  font-size: 18px; \n    box-shadow: 0 8px 6px 0px rgb(211, 211, 211);\n}\n\n.header >  div{\n  text-align: center;\n  /* border: 1px solid black; */\n  \n}\n\n.header a { \n  font-size: 30px;\n margin:10px; \n  padding: 10px;\n box-shadow: -2rem 0 3rem -2rem #000;\n}\n\n.profile a{\n\n  display: none;\n}\n\n.mobileMenu {\n  \n  font-size: 18px;\n  opacity: 1;\n  animation:  opacityOn 5s normal forwards;\n}\n\n@keyframes opacityOn {\n    0% {\n        opacity: 1;\n    }\n    25% {\n        opacity: 0;\n    }\n    50% {\n        opacity: 1;\n    }\n   \n    100% {\n        opacity: 1;\n    }\n}\n\n.mobileMenu div{\n  /* display: none; */\n  padding:20px 5px; \n  background: rgb(43, 37, 37); \n  border-bottom: white 1px solid;\n  color:white; \n\n}\n\n@media only screen and (max-width: 768px) {\n .profile a{\n\n  display: inline-block;\n}\n.profile ul{\n  display:none; \n}\n.menu {\n  display: none;\n}\n\n.header{\n  background: #42b983; \n  display: flex; \n  justify-content:space-between;\n  align-items: center;\n  font-size: 18px; \n  padding-left: 20px;\n  padding-right: 20px; \n  box-shadow: 0 8px 6px 0px rgb(211, 211, 211);\n}\n\n}\n\n\n</style>\n"]}]}